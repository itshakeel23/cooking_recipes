<h1>New forum_post</h1>

<% form_for(@forum_post) do |f| %>
  <%= f.error_messages %>


  <p>
    <%= f.label :forum_cat_l2_id %><br />
    <%= f.collection_select :forum_cat_l2_id, @forum_cat_l2s, :id, :title %>
  </p>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', forum_posts_path %>
